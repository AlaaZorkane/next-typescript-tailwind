{
  "name": "next-typescript-tailwind",
  "version": "2.0.0",
  "author": "Alaa Zorkane <https://github.com/alaazorkane>",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}'",
    "fix:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier --check src/**/*.js",
    "fix:prettier": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "next": "^9.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.7.2",
    "typescript": "3.7.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix --max-warnings=0 --cache"
    ]
